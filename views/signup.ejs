<% include ../index %>
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="./Css/sign.css">
    <script src="./javascript/index.js"></script>
</head>
<body>

<div class="jumbotron text-center">
  <h1>SignUp</h1>
</div>
  
<form action="" method="post">
    <div class="container" id="signup">
        <label for="Name">Name</label><br>
            <input type="text" name="" class="form-control" placeholder="Name"><br>
        
        <label for="Birthday">Birthday</label>
            <input type="date" name="" class="form-control" ><br>
        
        <label for="Mobile No">Mobile No</label>
            <input type="Num" name="" class="form-control" placeholder="Mobile No"><br>
        
        <label for="Email Id">Email Id</label>
            <input type="email" name="" class="form-control" placeholder="Email Id"><br>
        
        <label for="Password">Password</label>
            <input type="password" name="" id="pwd" class="form-control" value="123456" placeholder="Password"> 
        <input type="checkbox" name="" id="showPassword" onclick="Pwd()"> Show Password<br><br>

        <button type="submit" class="btn btn-success form-control">SIGNUP</button><br><br>
           <p class="text-center">Already have account? <a href="/login">Login here..</a></p>
    </div>
</form>
</body>
</html>
